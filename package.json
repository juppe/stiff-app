{
  "name": "stiff-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.7.0",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.2"
  },
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\"",
    "server": "nodemon --watch ../stiff-server/ ../stiff-server/server.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --config package.json 'src/**/*.{js,css}' --write",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001/"
}
